# Ignore bundler config.
/BackEnd/hospital-api/.bundle

# Ignore the default SQLite database.
/BackEnd/hospital-api/db/*.sqlite3
/BackEnd/hospital-api/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/BackEnd/hospital-api/log/*
/BackEnd/hospital-api/tmp/*
!/BackEnd/hospital-api/log/.keep
!/BackEnd/hospital-api/tmp/.keep

# Ignore uploaded files in development
/BackEnd/hospital-api/storage/*
!/BackEnd/hospital-api/storage/.keep

/BackEnd/hospital-api/.byebug_history

# Ignore master key for decrypting credentials and more.
/BackEnd/hospital-api/config/master.key


